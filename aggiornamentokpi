                    if (xPrev != x || yPrev != y || zPrev != z ){
            console.log("If s√¨");
            $(".up-left").append("<h4>object release at "+ ArrayHourMinute[0] + "</h4>");
            xPrev = x;
            yPrev = y;
            zPrev = z;
            setTimeout(function(){
                $(".up-left h4").remove();
            }, 6000);

        } else {
            $(".up-left h4").remove();
        }

        console.log("Updated!");